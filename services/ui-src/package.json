{
  "name": "ui-src",
  "description": "",
  "version": "1.0.0",
  "dependencies": {
    "@chakra-ui/react": "2.5.5",
    "@chakra-ui/styled-system": "^2",
    "@chakra-ui/toast": "2.1.0",
    "@cmsgov/design-system": "^13.0.0",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@hookform/resolvers": "^2.9.11",
    "@tanstack/react-query": "^5",
    "@tanstack/react-query-devtools": "^5",
    "@vitejs/plugin-react": "^4.3.1",
    "aws-amplify": "^6.12.3",
    "base-64": "^1.0.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^1.3.8",
    "dompurify": "^3.2.6",
    "file-saver": "^2.0.5",
    "framer-motion": "^4",
    "jest-launchdarkly-mock": "^2.1.0",
    "launchdarkly-react-client-sdk": "^3.4.0",
    "node-fetch": "3.3.2",
    "node-forge": "1.3.1",
    "object-path": "^0.11.8",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-dropzone": "^11.7.1",
    "react-hook-form": "^7.53.0",
    "react-icons": "^4.12.0",
    "react-idle-timer": "^4.6.4",
    "react-router-dom": "6.27.0",
    "react-textarea-autosize": "^8.5.3",
    "sass": "^1.80.4",
    "uuid": "^8.3.2",
    "vite": "^7.1.11",
    "vite-tsconfig-paths": "^4.3.2",
    "xml2js": "0.6.0"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "coverage": "jest --coverage",
    "test": "jest",
    "watchTest": "jest --watch",
    "postinstall": "patch-package"
  },
  "jest": {
    "testTimeout": 180000,
    "coverageReporters": [
      "json",
      [
        "lcov",
        {
          "projectRoot": "../../"
        }
      ],
      "text",
      "text-summary"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/measures/2021/**/*.{ts,tsx}",
      "!src/measures/2022/**/*.{ts,tsx}",
      "!src/measures/2023/**/*.{ts,tsx}",
      "!src/measures/2024/**/*.{ts,tsx}",
      "!src/measures/2025/**/*.{ts,tsx}"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "verbose": true,
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "<rootDir>/src/utils/testUtils/styleMock.ts",
      "^@ui-src/(.*)$": "<rootDir>/$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "globals": {
      "_env_": {}
    },
    "transformIgnorePatterns": [
      "node_modules/(?!axios)"
    ],
    "testEnvironment": "jest-environment-jsdom"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.716.0",
    "@babel/core": "^7.13.16",
    "@babel/plugin-transform-react-jsx": "^7.13.12",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.16.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^7.2.1",
    "@types/base-64": "^1.0.0",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^27.4.0",
    "@types/jest-axe": "^3.5.4",
    "@types/object-path": "^0.11.1",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-router-dom": "^5.3.2",
    "@types/uuid": "^8.3.3",
    "babel": "^6.23.0",
    "babel-jest": "^30.1.2",
    "jest": "^27.4.0",
    "jest-axe": "^6.0.0",
    "jest-environment-jsdom": "^27.4.0",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.4.1",
    "ts-jest": "^27.1.5",
    "typescript": "^5"
  },
  "resolutions": {
    "node-forge": "^1.3.1",
    "degenerator": "^4.0.4",
    "nth-check": "^2.0.1",
    "json5": "^2.2.3"
  }
}
